    <script>
        document.getElementById('rating').addEventListener('change', function () {
            // Remove yellow-option class from all options
            const options = this.options;
            for (let i = 0; i < options.length; i++) {
                options[i].classList.remove('yellow-option');
            }
            // Add yellow-option class to the selected option
            options[this.selectedIndex].classList.add('yellow-option');
        });

        // Trigger change event to apply initial color
        document.getElementById('rating').dispatchEvent(new Event('change'));
    </script>

    <script>
        // Select all elements with the "i" tag and store them in a NodeList called "stars"
        const stars = document.querySelectorAll(".stars i");

        // Loop through the "stars" NodeList
        stars.forEach((star, index1) => {
            // Add an event listener that runs a function when the "click" event is triggered
            star.addEventListener("click", () => {
                // Loop through the "stars" NodeList Again
                stars.forEach((star, index2) => {
                    // Add the "active" class to the clicked star and any stars with a lower index
                    // and remove the "active" class from any stars with a higher index
                    index1 >= index2 ? star.classList.add("active") : star.classList.remove("active");
                });
            });
        });
    </script>
    <script>
        // Function to format text into lines of 'wordsPerLine' words
        function formatText(text, wordsPerLine) {
            const words = text.split(' ');
            let formattedText = '';

            for (let i = 0; i < words.length; i += wordsPerLine) {
                formattedText += words.slice(i, i + wordsPerLine).join(' ') + ' \n';
            }

            return formattedText.trim();
        }

        // Get the element containing the comment content
        const commentElement = document.getElementById('comment-content');

        // Format the text and update the content
        if (commentElement) {
            const rawText = commentElement.textContent;
            const formattedText = formatText(rawText, 5);
            commentElement.textContent = formattedText;
        }
    </script>
